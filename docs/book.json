{
  "root": "./",
  "title": "HYPIQ Documentation",
  "description": "Advanced trading platform documentation for cryptocurrency markets",
  "author": "HYPIQ Team",
  "language": "en",
  "gitbook": "3.2.3",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-default",
    "search",
    "sharing",
    "fontsettings",
    "highlight",
    "lunr",
    "expandable-chapters",
    "sitemap-general",
    "copy-code-button"
  ],
  "pluginsConfig": {
    "sitemap-general": {
      "prefix": "https://docs.hypiq.finance/"
    },
    "sharing": {
      "twitter": true,
      "facebook": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["twitter"]
    },
    "fontsettings": {
      "theme": "white",
      "family": "sans",
      "size": 2
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "chapterMark": "pagebreak",
    "pageBreaksBefore": "/",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}